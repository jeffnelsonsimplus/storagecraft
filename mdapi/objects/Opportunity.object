    <validationRules>
        <fullName>Cannot_change_the_original_Est_Revenue</fullName>
        <active>true</active>
        <description>Cannot_change_the_original_Est_Revenue</description>
        <errorConditionFormula>AND(
  $Setup.ValidationController__c.ExecuteOpportunityValidation__c,
  NOT(
    ISBLANK(
      PRIORVALUE( EstRevenue__c )
    )
  ),
  ISCHANGED(EstRevenue__c )
)</errorConditionFormula>
        <errorDisplayField>EstRevenue__c</errorDisplayField>
        <errorMessage>Cannot change original estimated revenue</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_change_the_originalest_close_date</fullName>
        <active>true</active>
        <description>Cannot change the originalest close date</description>
        <errorConditionFormula>AND(
  $Setup.ValidationController__c.ExecuteOpportunityValidation__c,
  NOT(
    ISBLANK(
      PRIORVALUE(EstCloseDate__c)
    )
  ),
  ISCHANGED(EstCloseDate__c)
)</errorConditionFormula>
        <errorDisplayField>EstCloseDate__c</errorDisplayField>
        <errorMessage>Cannot change the original est close date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OnClose_Require_Reason</fullName>
        <active>true</active>
        <description>Reason for closing the Opportunity</description>
        <errorConditionFormula>AND(CONTAINS(TEXT(StageName), &quot;Closed&quot;), ISPICKVAL(Loss_Reason__c, &quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Loss_Reason__c</errorDisplayField>
        <errorMessage>Please select a Reason for closing the Opportunity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_Loss_Competitor</fullName>
        <active>true</active>
        <description>If lost opportunity, must have competitor set</description>
        <errorConditionFormula>AND(ISPICKVAL(StageName, &quot;Closed Lost&quot;), ISBLANK(Competitors__c))</errorConditionFormula>
        <errorDisplayField>Competitors__c</errorDisplayField>
        <errorMessage>Please select the competitor(s) contributed to losing this opportunity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_Loss_Competitor_Other</fullName>
        <active>true</active>
        <errorConditionFormula>AND(INCLUDES(Competitors__c, &quot;--other/new--&quot;), ISBLANK(OtherCompetitor__c))</errorConditionFormula>
        <errorDisplayField>OtherCompetitor__c</errorDisplayField>
        <errorMessage>Please enter the name of the other/new competitor</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_Loss_Details</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK(Details__c), OR(TEXT(StageName) = &quot;Duplicate&quot;, TEXT(StageName) = &quot;Cancelled&quot;, CONTAINS(TEXT(Loss_Reason__c), &quot;Lost&quot;)))</errorConditionFormula>
        <errorDisplayField>Details__c</errorDisplayField>
        <errorMessage>Please give details on the reason for loss, the duplicate record or why cancelled</errorMessage>
    </validationRules>

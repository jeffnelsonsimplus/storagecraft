    <fields>
        <fullName>SystemStatus__c</fullName>
        <externalId>false</externalId>
        <formula>if( 
ISBlank(Lead__c), 
&quot;nolead&quot;, 
if( 
Lead__r.IsConverted, 
if( 
ISBlank(EndUserOpportunity__c), 
&quot;noopp&quot;, 
if( 
EndUserOpportunity__r.IsClosed, 
if( 
EndUserOpportunity__r.IsWon, 
&quot;won&quot;, 
&quot;lost&quot; 
), 
&quot;working&quot; 
) 
), 
if( 
Lead__r.IsDisqualified__c, 
&quot;rejected&quot;, 
&quot;new&quot; 
) 
) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>System Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
